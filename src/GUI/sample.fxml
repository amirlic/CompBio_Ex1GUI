    <VBox>
        <Button fx:id="makeMove" text="make move" alignment="CENTER" onAction="#makeMove"/>

        <VBox>
            <Text text="Global Tree:"></Text>
            <Text fx:id="globalTree" text="blaGlobal Tree:"></Text>
        </VBox>

        <VBox>
            <Text text="Global Fire:"></Text>
            <Text fx:id="globalFire" text="blaGlobal Tree:"></Text>
        </VBox>

        <VBox>
            <Text text="Global Empty:"></Text>
            <Text fx:id="globalEmpty" text="blaGlobal Tree:"></Text>
        </VBox>

        <VBox>
            <Text text="Global index:"></Text>
            <Text fx:id="globalIndex" text="blaGlobal Tree:"></Text>
        </VBox>

        <VBox>
            <Text text="Local index:"></Text>
            <Text fx:id="localIndex" text="blaGlobal Tree:"></Text>
        </VBox>

    </VBox>
</HBox>